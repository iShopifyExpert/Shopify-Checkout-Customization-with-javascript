//Add this code in google anylatic section from settings
var fileref=document.createElement("link");fileref.setAttribute("rel", "stylesheet");fileref.setAttribute("type", "text/css");fileref.setAttribute("href", "https://cdn.shopify.com/s/files/1/0038/1245/8609/t/2/assets/checkout.css?aaliyan20502");document.getElementsByTagName("head")[0].appendChild(fileref); document.getElementsByClassName("order-summary__sections")[0].insertAdjacentHTML("beforeend",'<div class="leftside"><ul><li><a href="/pages/shipping">Shipping</a></li><li><a href="/pages/returns-and-exchanges">Returns & Exchanges</a></li><li><a href="/pages/faqs">Faqs</a></li></ul></div><div class="rightside"><ul><li><a href="/pages/terms-of-sale">Terms of Sale</a></li><li><a href="/pages/legal-notice">Legal Notice</a></li><li><a href="#">Customer Services</a></li></ul></div>');
window.onload = imgcode; function imgcode(){ var elements = document.querySelectorAll('.product-thumbnail__image'); Array.prototype.forEach.call(elements, function(el, i){ var src = el.src; var rep = src.replace("_small", "_large"); el.src = rep; });}
